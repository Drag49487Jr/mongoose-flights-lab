<%- include('../partials/header') %>
    <table>
        <thead>
            <tr>
                <th><%= flights.airline %></th>
                <th>Airline</th>
                <th>FlightNo</th>
                <th>Departs</th>
            </tr>
        </thead>
        <tbody>
            <% flights.forEach(function(f) { %>
                <tr>
                    <td><%= f.flightNo %></td>
                    <td><%= f.departs %></td>
                </tr>
                <% }); %>
        </tbody>
    </table>

<%- include('../partials/footer') %>