<%- include('../partials/header') %>
<table>
    <thead>
        <tr>
            <th>Airline</th>
            <th>FlightNo</th>
            <th>Departs</th>
        </tr>
    </thead>
    <tbody>
        <% flights.forEach(function(f) { %>
            <tr>
                <td><%= f.airline %></td>
                <td><%= f.flightNo %></td>
                <td><%= f.departs %></td>
            </tr>
            <td><a href = "/flights/<%= f._id %>">Details</a></td>
        <% }); %>
    </tbody>
</table>

<%- include('../partials/footer') %>